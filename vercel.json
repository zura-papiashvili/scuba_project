{
  "version": 2,
  "builds": [
    {
      "functions": {
        "manage.py": {
          "maxDuration": 60
        }
      },
      "src": "scuba_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "vercel-build.sh",
      "use": "@vercel/python",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "scuba_project/wsgi.py"
    }
  ],
  "outputDirectory": "staticfiles"
}